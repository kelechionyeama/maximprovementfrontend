{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"typeRoots": ["./types/env.d.tsx"],
		"paths": {
			"@/*": [
				"./*"
			],
			"@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	]
}